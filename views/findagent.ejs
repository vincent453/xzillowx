    <style>
        * {
            margin: 0;
            padding: 0;
            box-sizing: border-box;
            font-family: 'Arial', sans-serif;
        }
        
        body {
            color: #2a2a2a;
            line-height: 1.6;
        }
        
        /* Header Styles */
        header {
            display: flex;
            justify-content: space-between;
            align-items: center;
            padding: 15px 40px;
            border-bottom: 1px solid #e5e5e5;
        }
        
        .logo {
            font-size: 24px;
            font-weight: bold;
            color: #0066cc;
        }
        
        .nav-primary {
            display: flex;
            gap: 20px;
        }
        
        .nav-primary a {
            text-decoration: none;
            color: #2a2a2a;
        }
        
        .nav-secondary {
            display: flex;
            gap: 20px;
        }
        
        .nav-secondary a {
            text-decoration: none;
            color: #2a2a2a;
        }
        
        /* Category Navigation */
        .category-nav {
            display: flex;
            justify-content: flex-start;
            gap: 20px;
            padding: 10px 40px;
            border-bottom: 1px solid #e5e5e5;
            background-color: #f8f8f8;
        }
        
        .category-nav a {
            text-decoration: none;
            color: #2a2a2a;
            font-size: 14px;
        }
        
        .category-nav a.active {
            color: #0066cc;
            font-weight: bold;
        }
        
        /* Hero Section */
        .hero {
            background-image: linear-gradient(rgba(0,0,0,0.5), rgba(0,0,0,0.5)), url('/image/agent_banner.webp');
            background-size: cover;
            background-position: center;
            color: white;
            padding: 60px 40px;
            text-align: center;
            height: 32rem;
        }
        
        .hero h1 {
            font-size: 32px;
            margin-top: 60px;
        }
        
        /* Search Box */
        .search-container {
            max-width: 600px;
            margin: 0 auto;
            background-color: white;
            border-radius: 5px;
            margin-top: 30px;
            padding: 15px;
        }
        
        .search-tabs {
            display: flex;
            gap: 15px;
            margin-bottom: 15px;
        }
        
        .search-tabs button {
            background: none;
            border: none;
            padding: 5px 10px;
            cursor: pointer;
            font-weight: bold;
        }
        
        .search-tabs button.active {
            border-bottom: 2px solid #0066cc;
            color: #0066cc;
        }
        
        .search-box {
            display: flex;
            border: 1px solid #ddd;
            border-radius: 4px;
            overflow: hidden;
        }
        
        .search-box input {
            flex-grow: 1;
            padding: 10px;
            border: none;
            outline: none;
        }
        
        .search-box button {
            background-color: #0066cc;
            color: white;
            border: none;
            padding: 10px 15px;
            cursor: pointer;
        }
        
        /* Main Content */
        .main-content-2 {
            padding: 40px;
        }
        
        .location-title {
            margin-bottom: 20px;
        }
        
        .location-description {
            margin-bottom: 30px;
            color: #555;
        }
        
        /* Professional Grid */
        .professionals-grid {
            display: grid;
            grid-template-columns: repeat(2, 1fr);
            gap: 20px;
            margin-bottom: 30px;
        }
        
        .professional-card {
            border: 1px solid #ddd;
            border-radius: 8px;
            padding: 20px;
            display: flex;
            gap: 20px;
            position: relative;
        }
        
        .professional-image {
            width: 150px;
            height: 150px;
            border-radius: 100%;
            overflow: hidden;
            flex-shrink: 0;
        }
        
        .professional-details {
            flex-grow: 1;
        }
        
        .professional-name {
            font-weight: bold;
            font-size: 18px;
            margin-bottom: 5px;
        }
        
        .professional-company {
            color: #555;
            margin-bottom: 10px;
        }
        
        .professional-stats {
            display: flex;
            gap: 20px;
            margin-bottom: 10px;
        }
        
        .rating {
            position: absolute;
            top: 20px;
            right: 20px;
            display: flex;
            align-items: center;
            gap: 5px;
        }
        
        .badge {
            background-color: #0066cc;
            color: white;
            border-radius: 50%;
            width: 24px;
            height: 24px;
            display: flex;
            align-items: center;
            justify-content: center;
            font-size: 12px;
            position: absolute;
            bottom: 20px;
            left: 20px;
        }
        
        /* Help Finding Section */
        .help-section {
            background-color: #f8f8f8;
            padding: 30px 40px;
            margin-bottom: 30px;
            display: flex;
            align-items: center;
            gap: 20px;
        }
        
        .help-content {
            flex-grow: 1;
        }
        
        .help-cta {
            display: inline-block;
            background-color: #0066cc;
            color: white;
            padding: 10px 20px;
            text-decoration: none;
            border-radius: 4px;
            margin-top: 10px;
        }
        
        /* Full Width Banner */
        .banner {
            background-image: linear-gradient(rgba(0,0,0,0.5), rgba(0,0,0,0.5)), url('/image/help_agent.jpg');
            background-size: cover;
            background-position: center;
            color: white;
            padding: 60px 40px;
          
            height: 20rem;
        }
        
        .banner-content3 {
            max-width: 600px;
        }
        
        .banner h2 {
            margin-bottom: 20px;
        }
        
        /* FAQ Section */
        .faq-section {
            background-color: #2a2a2a;
            color: white;
            padding: 40px;
        }
        
        .faq-title {
            margin-bottom: 30px;
            text-align: center;
        }
        
        .faq-item {
            border-top: 1px solid rgba(255,255,255,0.2);
            padding: 20px 0;
            display: flex;
            justify-content: space-between;
            align-items: center;
            cursor: pointer;
        }
        
        /* Resources Section */
        .resources-section {
            padding: 40px;
            background-color: #f8f8f8;
        }
        
        .resources-title {
            margin-bottom: 20px;
        }
        
        .resources-content {
            margin-bottom: 30px;
        }
        
        /* Category Lists */
        .category-lists {
            display: grid;
            grid-template-columns: repeat(4, 1fr);
            gap: 20px;
            margin-bottom: 40px;
        }
        
        .category-list {
            margin-bottom: 30px;
        }
        
        .category-list h4 {
            margin-bottom: 15px;
            color: #0066cc;
        }
        
        .category-list ul {
            list-style: none;
        }
        
        .category-list li {
            margin-bottom: 10px;
        }
        
        .category-list a {
            text-decoration: none;
            color: #2a2a2a;
        }
        
        .show-more {
            color: #0066cc;
            text-decoration: none;
            display: flex;
            align-items: center;
            gap: 5px;
            margin-top: 10px;
        }
        
        /* Footer */
        footer {
            background-color: #f8f8f8;
            padding: 40px;
            border-top: 1px solid #e5e5e5;
        }
        
        .footer-links {
            display: flex;
            flex-wrap: wrap;
            gap: 20px;
            margin-bottom: 30px;
        }
        
        .footer-links a {
            text-decoration: none;
            color: #555;
            font-size: 14px;
        }
        
        .footer-disclaimer {
            font-size: 12px;
            color: #777;
            margin-bottom: 30px;
        }
        
        .app-badges {
            display: flex;
            gap: 15px;
            margin-bottom: 30px;
        }
        
        .app-badge {
            height: 40px;
        }
        
        .social-media {
            display: flex;
            gap: 15px;
            margin-bottom: 30px;
        }
        
        .social-icon {
            width: 30px;
            height: 30px;
            border-radius: 50%;
            background-color: #0066cc;
            display: flex;
            align-items: center;
            justify-content: center;
            color: white;
        }
        
        .copyright {
            font-size: 12px;
            color: #777;
        }
        
        .footer-icon {
            color: #0066cc;
            font-size: 24px;
            margin-right: 10px;
        }
        
        /* Building Icons */
        .building-icons {
            display: flex;
            justify-content: space-between;
            margin-top: 40px;
        }
        
        .building-icon {
            text-align: center;
            color: #0066cc;
        }
        @media screen and (max-width: 991px) {
    /* Header adjustments */
    header {
        padding: 15px 20px;
    }
    
    .category-nav {
        padding: 10px 20px;
        overflow-x: auto;
        white-space: nowrap;
    }
    
    /* Hero section adjustments */
    .hero {
        padding: 40px 20px;
        height: 28rem;
    }
    
    .hero h1 {
        font-size: 28px;
    }
    
    /* Main content adjustments */
    .main-content-2 {
        padding: 30px 20px;
    }
    
    /* Professional grid change to single column */
    .professionals-grid {
        grid-template-columns: 1fr;
    }
    
    /* Category lists change to two columns */
    .category-lists {
        grid-template-columns: repeat(2, 1fr);
    }
}

/* Mobile Layout (up to 767px) */
@media screen and (max-width: 767px) {
    /* Header simplification */
    .nav-primary, .nav-secondary {
        display: none;
    }
    
    .logo {
        font-size: 20px;
    }
    
    /* Hero section adjustments */
    .hero {
        padding: 30px 15px;
        height: 24rem;
    }
    
    .hero h1 {
        font-size: 24px;
        margin-top: 30px;
    }
    
    .search-container {
        margin-top: 20px;
    }
    
    /* Professional cards layout adjustment */
    .professional-card {
        flex-direction: column;
        align-items: center;
        text-align: center;
    }
    
    .professional-image {
        margin-bottom: 15px;
    }
    
    .rating {
        position: static;
        justify-content: center;
        margin-top: 15px;
    }
    
    .badge {
        top: 20px;
        left: 20px;
        bottom: auto;
    }
    
    /* Help section adjustment */
    .help-section {
        padding: 20px 15px;
    }
    
    /* Banner adjustment */
    .banner {
        padding: 30px 15px;
        height: auto;
        min-height: 18rem;
    }
    
    /* Category lists change to single column */
    .category-lists {
        grid-template-columns: 1fr;
    }
    
    /* Footer adjustments */
    footer {
        padding: 30px 15px;
    }
    
    .footer-links {
        flex-direction: column;
        gap: 10px;
    }
}

/* Small Mobile Layout (up to 480px) */
@media screen and (max-width: 480px) {
    /* Further header simplification */
    header {
        padding: 10px 15px;
    }
    
    .logo {
        font-size: 18px;
    }
    
    /* Hero section further reduction */
    .hero {
        height: 20rem;
    }
    
    .hero h1 {
        font-size: 20px;
    }
    
    /* Search container adjustments */
    .search-tabs {
        flex-wrap: wrap;
    }
    
    /* Professional card adjustments */
    .professional-image {
        width: 120px;
        height: 120px;
    }
    
    .professional-stats {
        flex-direction: column;
        gap: 5px;
    }
    
    /* Mobile menu adjustments */
    .mobile-menu {
        width: 100%;
    }
}

/* Specific adjustments for existing mobile menu */
@media screen and (min-width: 992px) {
    .mobile-nav {
        display: none;
    }
    
    .mobile-menu {
        display: none;
    }
}

@media screen and (max-width: 991px) {
    .desk {
        display: none;
    }
    
    .hide-major {
        display: none;
    }
    
    /* Ensure mobile menu works properly */
    .mobile-menu.active {
        display: block;
        position: fixed;
        top: 0;
        left: 0;
        width: 80%;
        height: 100%;
        background-color: white;
        z-index: 1000;
        overflow-y: auto;
    }
    
    .overlay.active {
        display: block;
        position: fixed;
        top: 0;
        left: 0;
        width: 100%;
        height: 100%;
        background-color: rgba(0,0,0,0.5);
        z-index: 999;
    }
}
