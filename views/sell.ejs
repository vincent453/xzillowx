    <style>
      :root {
        --primary-color: #01265a;
        --text-color: #2a2a33;
        --light-gray: #f9f9fb;
        --border-color: #d1d1d5;
      }

      * {
        box-sizing: border-box;
        margin: 0;
        padding: 0;
        font-family: "Open Sans", Arial, sans-serif;
      }

      body {
        color: var(--text-color);
        line-height: 1.5;
      }

      .container {
        max-width: 90%;
        margin: 0 auto;
      }

      /* Header Styles */
      header {
        border-bottom: 1px solid var(--border-color);
        padding: 12px 0;
      }

      .header-container {
        display: flex;
        justify-content: space-between;
        align-items: center;
      }

      .logo {
        color: var(--primary-color);
        font-weight: bold;
        font-size: 24px;
        display: flex;
        align-items: center;
      }

      .logo svg {
        height: 24px;
        margin-right: 5px;
      }

      nav ul {
        display: flex;
        list-style: none;
      }

      nav li {
        margin-left: 24px;
      }

      nav a {
        text-decoration: none;
        color: var(--text-color);
        font-size: 14px;
      }

      .sign-in {
        font-weight: bold;
        color: var(--primary-color);
      }

      /* Hero Section */
      .hero {
        background-image: linear-gradient(
            rgba(0, 0, 0, 0.3),
            rgba(0, 0, 0, 0.3)
          ),
          url("/image/banner_sell.jpg");
        background-size: cover;
        background-position: center;
        color: white;
        padding: 60px 0;
        height: 30rem;
        text-align: center;
      }

      .hero h1 {
        font-size: 40px;
        margin-bottom: 16px;
      }

      .hero p {
        font-size: 18px;
        max-width: 600px;
        margin: 0 auto 24px;
      }

      .hero-button {
        background-color: var(--primary-color);
        color: white;
        padding: 12px 24px;
        border: none;
        font-weight: bold;
        cursor: pointer;
        text-decoration: none;
        display: inline-block;
      }

      /* Main Content */
      .selling-options {
        padding: 40px 0;
      }

      .section-title {
        font-size: 28px;
        margin-bottom: 30px;
      }

      .option-cards {
        display: flex;
        gap: 30px;
        margin-bottom: 40px;
      }

      .option-card {
        flex: 1;
        border: 1px solid var(--border-color);
        border-radius: 8px;
        padding: 24px;
        display: flex;
        flex-direction: column;
      }

      .option-card h3 {
        font-size: 20px;
        margin-bottom: 16px;
        display: flex;
        align-items: center;
      }

      .option-icon {
        width: 50px;
        height: 50px;
        margin-right: 16px;
        background-color: #f0f8ff;
        border-radius: 8px;
        display: flex;
        align-items: center;
        justify-content: center;
      }

      .option-description {
        font-size: 14px;
        flex-grow: 1;
        margin-bottom: 20px;
      }

      .option-button {
        background-color: var(--primary-color);
        color: white;
        padding: 12px;
        border: none;
        border-radius: 4px;
        font-weight: bold;
        cursor: pointer;
        text-decoration: none;
        display: inline-block;
        text-align: center;
        width: 100%;
      }

      /* Selling Methods Sections */
      .selling-method {
        padding: 40px 0;
        border-top: 1px solid var(--border-color);
      }

      .method-container {
        display: flex;
        gap: 40px;
        align-items: center;
      }

      .method-text {
        flex: 1;
      }

      .method-image {
        flex: 1;
        display: flex;
        justify-content: center;
      }

      .method-image img {
        max-width: 50%;
        height: auto;
        border-radius: 8px;
      }

      .method-steps {
        margin-top: 30px;
        display: flex;
        gap: 30px;
      }

      .steps-list {
        flex: 1;
      }

      .how-to {
        flex: 1;
      }

      .steps-list ul {
        list-style-type: none;
      }

      .steps-list li {
        display: flex;
        align-items: center;
        margin-bottom: 12px;
        font-size: 14px;
      }

      .check-icon {
        color: var(--primary-color);
        margin-right: 10px;
        font-weight: bold;
      }

      .how-to h4 {
        margin-bottom: 12px;
      }

      .how-to p {
        font-size: 14px;
        margin-bottom: 16px;
      }

      /* Resources Section */
      .resources {
        background-image: url("/image/explore_home.jpg");
        padding: 100px 0;
        height: 25rem;
        background-color: var(--light-gray);
        text-align: center;
      }

      .resources-title {
        font-size: 28px;
        margin-bottom: 20px;
      }

      .resources-subtitle {
        font-size: 16px;
        margin-bottom: 30px;
        max-width: 600px;
        margin-left: auto;
        margin-right: auto;
      }

      .search-container {
        max-width: 600px;
        margin: 0 auto;
        display: flex;
      }

      .search-input {
        flex-grow: 1;
        padding: 12px;
        border: 1px solid var(--border-color);
        border-right: none;
        border-radius: 4px 0 0 4px;
      }

      .search-button {
        background-color: var(--primary-color);
        color: white;
        padding: 12px 24px;
        border: none;
        border-radius: 0 4px 4px 0;
        font-weight: bold;
        cursor: pointer;
      }

      /* Process Section */
      .process {
        padding: 60px 0;
        text-align: center;
      }

      .process-title {
        font-size: 24px;
        margin-bottom: 40px;
      }

      .process-steps {
        display: flex;
        justify-content: space-between;
        margin-bottom: 50px;
      }

      .process-step {
        flex: 1;
        padding: 0 15px;
        position: relative;
      }

      .process-step:not(:last-child)::after {
        content: "";
        position: absolute;
        top: 35px;
        right: -15px;
        width: 30px;
        height: 1px;
        background-color: var(--border-color);
      }

      .step-number {
        width: 70px;
        height: 70px;
        border-radius: 50%;
        background-color: var(--light-gray);
        display: flex;
        align-items: center;
        justify-content: center;
        margin: 0 auto 20px;
        font-weight: bold;
        color: var(--primary-color);
      }

      .step-title {
        font-size: 16px;
        margin-bottom: 10px;
      }

      /* Home Examples */
      .home-examples {
        display: flex;
        gap: 20px;
        margin-bottom: 40px;
        justify-content: center;
      }

      .home-card {
        flex: 1;
        max-width: 250px;
        border: 1px solid var(--border-color);
        border-radius: 8px;
        overflow: hidden;
      }

      .home-image1 {
        height: 150px;
        background-color: #eee;
        background-image: url("/image/3debf302670ccf99512db3eadc4bb0e4-p_e.webp");
        background-size: cover;
      }
      .home-image2 {
        height: 150px;
        background-color: #eee;
        background-image: url("/image/26d1c89a11236da9c4db1740eb72eb49-p_e.webp");
        background-size: cover;
      }
      .home-image3 {
        height: 150px;
        background-color: #eee;
        background-image: url("/image/94f8decded5f4160956fdb73eeed6b7b-p_e.webp");
        background-size: cover;
      }
      .home-image4 {
        height: 150px;
        background-color: #eee;
        background-image: url("/image/70372e4e2904f279fbc9dd7c29fc8cfe-p_e.webp");
        background-size: cover;
      }

      .home-details {
        padding: 15px;
        font-size: 14px;
      }

      .home-price {
        font-weight: bold;
        font-size: 18px;
        margin-bottom: 5px;
      }

      .home-specs {
        display: flex;
        margin-bottom: 10px;
      }

      .home-specs span {
        margin-right: 10px;
        font-size: 12px;
        color: #666;
      }

      .home-address {
        font-size: 12px;
        color: #666;
      }

      .view-all-button {
        background-color: var(--primary-color);
        color: white;
        padding: 12px 24px;
        border: none;
        border-radius: 4px;
        font-weight: bold;
        cursor: pointer;
      }

      /* FAQ Section */
      .faq {
        padding: 60px 0;
        border-top: 1px solid var(--border-color);
      }

      .faq-title {
        font-size: 28px;
        margin-bottom: 30px;
        text-align: center;
      }

      .faq-item {
        border-bottom: 1px solid var(--border-color);
        padding: 20px 0;
      }

      .faq-question {
        display: flex;
        justify-content: space-between;
        align-items: center;
        cursor: pointer;
        font-weight: bold;
        font-size: 16px;
      }

      .faq-answer {
        margin-top: 15px;
        font-size: 14px;
        display: none;
      }

      .expand-icon {
        font-size: 20px;
      }

      .faq-item.active .faq-answer {
        display: block;
      }

      /* Footer */
      footer {
        background-color: var(--light-gray);
        padding: 40px 0;
        font-size: 14px;
      }

      .footer-links {
        display: flex;
        justify-content: space-between;
        margin-bottom: 30px;
      }

      .footer-column {
        flex: 1;
      }

      .footer-column h4 {
        margin-bottom: 15px;
        font-size: 16px;
      }

      .footer-column ul {
        list-style: none;
      }

      .footer-column li {
        margin-bottom: 8px;
      }

      .footer-column a {
        text-decoration: none;
        color: #666;
      }

      .footer-bottom {
        text-align: center;
        padding-top: 30px;
        border-top: 1px solid var(--border-color);
      }

      .social-icons {
        display: flex;
        justify-content: center;
        margin: 20px 0;
      }

      .social-icon {
        width: 30px;
        height: 30px;
        border-radius: 50%;
        background-color: #ddd;
        margin: 0 5px;
        display: flex;
        align-items: center;
        justify-content: center;
      }

      .footer-disclaimer {
        font-size: 12px;
        color: #666;
        max-width: 800px;
        margin: 0 auto;
      }

      /* Responsive Design */
      @media (max-width: 768px) {
        .option-cards,
        .method-container,
        .method-steps,
        .process-steps,
        .home-examples,
        .footer-links {
          flex-direction: column;
        }

        .option-card,
        .home-card {
          max-width: 100%;
        }

        .method-image {
          margin-top: 30px;
        }

        .process-step:not(:last-child)::after {
          display: none;
        }

        nav ul {
          display: none;
        }
      }
    </style>
  </head>
