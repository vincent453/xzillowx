    .search-input {
      width: 100%;
      padding: 12px 15px;
      border: 1px solid #ccc;
      border-radius: 5px;
      font-size: 16px;
    }

    .search-button {
      position: absolute;
      right: 10px;
      top: 50%;
      transform: translateY(-50%);
      background: none;
      border: none;
      cursor: pointer;
    }

    .button {
      background-color: #0068d9;
      color: white;
      border: none;
      border-radius: 5px;
      padding: 15px 25px;
      font-size: 16px;
      cursor: pointer;
      font-weight: bold;
    }

    .modal-overlay {
      display: none;
      position: fixed;
      top: 0;
      left: 0;
      right: 0;
      bottom: 0;
      background-color: rgba(0, 0, 0, 0.6);
      z-index: 1000;
    }

    .modal {
      display: none;
      position: fixed;
      top: 50%;
      left: 50%;
      transform: translate(-50%, -50%);
      background-color: rgb(246, 246, 250);
      width: 90%;
      max-width: 500px;
      border-radius: 20px;
      z-index: 1001;
      max-height: 90vh;
      overflow-y: auto;
    }

    .modal-header {
      display: flex;
      justify-content: space-between;
      align-items: center;
      padding: 15px 20px;
      border-bottom: 1px solid #eee;
    }

    .modal-title {
      font-weight: bold;
      font-size: 20px;
    }

    .close-button {
      background: none;
      border: none;
      font-size: 24px;
      cursor: pointer;
    }

    .modal-content {
      padding: 20px;
    }

    .intro-section {
      background-color: white;
      padding: 20px;
      margin-bottom: 15px;
      border-radius: 5px;
    }

    .intro-title {
      font-size: 20px;
      font-weight: bold;
      margin-bottom: 15px;
      color: #333;
    }

    .intro-text {
      color: #444;
      line-height: 1.5;
      margin-bottom: 10px;
    }

    .form-section {
      background-color: #f9f9f9;
      padding: 20px;
      border-radius: 5px;
      margin-bottom: 15px;
    }

    .form-title {
      font-size: 18px;
      font-weight: bold;
      margin-bottom: 20px;
      color: #333;
    }

    .form-field {
      margin-bottom: 20px;
    }

    .form-label {
      display: block;
      margin-bottom: 10px;
      font-weight: bold;
    }

    .required {
      color: red;
    }

    .form-select,
    .form-input {
      width: 100%;
      padding: 12px;
      border: 1px solid #ccc;
      border-radius: 5px;
      font-size: 16px;
      background-color: white;
    }

    .income-input,
    .debt-input {
      position: relative;
    }

    .income-input input,
    .debt-input input {
      padding-left: 30px;
    }

    .currency-symbol {
      position: absolute;
      left: 12px;
      top: 50%;
      transform: translateY(-50%);
      color: #333;
      font-weight: bold;
    }

    .time-period {
      position: absolute;
      right: 12px;
      top: 50%;
      transform: translateY(-50%);
      color: #777;
    }

    .helper-text {
      font-size: 14px;
      color: #666;
      margin-top: 5px;
    }

    .submit-button {
      width: 100%;
      padding: 15px;
      background-color: #5d88ff;
      color: white;
      border: none;
      border-radius: 5px;
      font-size: 16px;
      font-weight: bold;
      cursor: pointer;
      margin-top: 20px;
    }

    .submit-button:hover {
      background-color: #4a6ecc;
    }

    .home-images {
      display: flex;
      justify-content: space-between;
      margin-top: 20px;
    }

    .home-image {
      width: 48%;
      border-radius: 8px;
      overflow: hidden;
    }

    .home-image img {
      width: 100%;
      height: auto;
    }
    
    .debt-info {
      font-size: 13px;
      color: #666;
      margin-top: 8px;
      line-height: 1.4;
    }

    </style>
