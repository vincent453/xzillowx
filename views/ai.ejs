/* Base styles */
.partner-content {
    width: 100%;
    background-color: rgb(249, 249, 251);
    display: flex;
    justify-content: center;
    padding: 4rem 0;
}

.blue-img img {
    width: 10rem;
}

.footer-art {
    margin-top: 5rem;
}

nav {
    border-bottom: 1px solid rgb(205, 205, 211);
}

.partner-with-xzillow {
    width: 60rem; /* Fixed width for larger screens */
    display: flex;
    flex-direction: column;
    align-items: center;
    row-gap: 2rem;
    padding: 4rem 0;
    height: auto;
}

.partner-with-xzillow h1 {
    font-family: "Gideon Roman", serif;
    font-size: 46px;
    line-height: 55.2px;
    font-weight: 700;
}

.partner-with-xzillow p {
    font-size: 16px;
    font-weight: 400;
}

.partner-with-xzillow .line {
    background-color: rgb(255, 210, 55);
    width: 144px;
    height: 2px;
}

.partner-with-xzillow h3 {
    font-size: 24px;
    font-weight: 700;
}

/* Fix the card layout */
.card-parent {
    display: flex;
    flex-direction: column;
    margin-top: 2rem;
}

.section-header {
    display: flex;
    justify-content: space-between;
    align-items: center;
    width: 90%;
    max-width: 1200px;
    margin: 3rem auto 2rem;
}

.section-header h1 {
    font-size: 2rem;
}

.cards-container {
    display: flex;
    justify-content: center;
    align-items: center;
    gap: 2rem;
    margin: 0 auto 4rem;
    flex-wrap: wrap;
    max-width: 100%;
}

.cards {
    padding: 0px;
    display: flex;
    justify-content: center;
}

.cards-container .cards .card {
    width: 400px;
    height: 408px;
    box-shadow: 0 0 10px 0px #605d5d7a;
    background-color: #fffffc;
    display: flex;
    flex-direction: column;
    align-items: center;
    overflow: hidden;
   
}

.card .text-content {
    display: flex;
    justify-content: space-between;
    padding: 2rem;
    row-gap: 20px;
    flex-direction: column;
    align-items: flex-start;
    width: 90%;
}


.card img {
    width: 100%;
    height: 186px;
    object-fit: cover;
}

body {
    background-color: #F9F9FB;
}

h1 {
    font-family: "Gideon Roman", serif;
}

.zai {
    margin-top: 4rem;
}

.check-out {
    width: 12rem;
    height: 3rem;
    border: none;
    color: #ffffff;
    font-weight: 400;
    background-color: #006aff;
    border-radius: 5px;
    font-size: 1rem;
    margin-top: 1rem;
    cursor: pointer;
    transition: background-color 0.2s;
}

.check-out:hover {
    background-color: #0056cc;
}

.feat {
    display: flex;
    align-items: center;
    gap: 1rem;
    flex-wrap: wrap;
}

.feat button {
    padding: 8px 13px;
    background-color: #f2a619;
    color: #fff;
    border: 0;
    text-transform: uppercase;
    font-size: 11px;
    font-weight: 800;
    white-space: nowrap;
}

.feat a {
    text-decoration: none;
    color: #6c6767;
    font-size: 14px;
}

.text-content h2 {
    font-weight: 600;
    font-size: 26px;
    margin: 0;
    padding: 0;
}

.work-at-xzillow {
    background-color: #1b5098;
    width: 100%;
    padding: 4rem 0;
    display: flex;
    justify-content: center;
    margin-top: 2rem;
}

.fea-rea {
    display: flex;
    justify-content: center;
    margin-top: 3rem;
    gap: 4rem;
    flex-wrap: wrap;
    max-width: 1200px;
    margin-left: auto;
    margin-right: auto;
}

.featured, .recent {
    flex: 1;
    min-width: 10%;
}

.featured p {
    line-height: 2;
}

.recent p {
    line-height: 2;
}

.work-at-xzillow .content {
    display: flex;
    justify-content: center;
    align-items: center;
    flex-direction: column;
    row-gap: 1rem;
}

.content h2 {
    color: #fff;
    font-size: 2rem;
    text-align: center;
}

.content p {
    color: #fff;
    text-align: center;
}

/* Media Query for Medium Devices (max-width: 768px) */
@media screen and (max-width: 768px) {
    .section-header {
        flex-direction: column;
        text-align: center;
        gap: 1rem;
    }
    
    .partner-with-xzillow h1 {
        font-size: 2.5rem;
        text-align: center;
    }
    
    .cards-container {
        flex-direction: column;
        align-items: center;
    }
    
    .fea-rea {
        padding: 0 2rem;
    }
}

/* Media Query for Small Devices (max-width: 480px) */
@media screen and (max-width: 480px) {
    /* Navigation */
    .nav-content {
        flex-direction: column;
    }
    
  
    
    .feat {
        flex-direction: column;
        align-items: flex-start;
    }
    
    .first-ul, .second-ul {
        display: none;
    }
    
    .logo-con {
        margin: 1rem auto;
    }
    
    .logo-con img {
        width: 40px;
    }
    
    .logo-con h2 {
        font-size: 1.2rem;
    }
    
    /* Partner section */
    .partner-with-xzillow {
        width: 90%;
    }
    
    .partner-with-xzillow h1 {
        font-size: 2rem;
        text-align: center;
    }
    
    .check-out {
        width: 80%;
        margin: 2rem auto;
        display: block;
    }
    
    /* Cards section */
    .cards-container {
        gap: 2rem;
        margin-bottom: 2rem;
    }
    
    .cards-container .cards .card {
        width: 90%;
        max-width: 320px;
        height: auto;
        margin: 0 auto;
    }
    
    .card img {
        height: auto;
    }
    
    .section-header h1 {
        font-size: 1.5rem;
    }
    
    /* Work at XZillowx section */
    .work-at-xzillow {
        width: 100%;
        height: auto;
        padding: 2rem 0;
    }
    
    .content h2 {
        font-size: 1.5rem;
    }
    
    /* Featured & Recent section */
    .fea-rea {
        flex-direction: column;
        margin: 2rem 1rem;
        gap: 3rem;
    }
    
    /* Footer */
    .footer-container {
        padding: 1rem;
    }
    
    .links-row {
        flex-direction: column;
        gap: 0.5rem;
    }
    
    .bottom-row {
        flex-direction: column;
        align-items: center;
        gap: 1rem;
    }
}

/* Media Query for Extra Small Devices (max-width: 360px) */
@media screen and (max-width: 360px) {
    .partner-with-xzillow h1 {
        font-size: 1.5rem;
    }
    
    .text-content h2 {
        font-size: 1rem;
    }
    
    .feat a {
        font-size: 12px;
    }
    
    .content h2 {
        font-size: 1.2rem;
    }
}
